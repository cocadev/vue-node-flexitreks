{
  "name": "flexitreks-api",
  "version": "0.0.1",
  "description": "Flexitreks API",
  "private": true,
  "main": "src/app.js",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "NODE_ENV=live node dist/app.js",
    "dev": "nodemon src/app.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "mocha src/tests --compilers js:babel-core/register --timeout 5000",
    "seed": "knex seed:make --knexfile ./src/knexfile.js --",
    "seed:test": "NODE_ENV=test knex seed:make --knexfile ./src/knexfile.js --",
    "seed:test:run": "NODE_ENV=test knex seed:run --knexfile ./src/knexfile.js",
    "seed:run": "knex seed:run --knexfile ./src/knexfile.js",
    "migrate": "knex migrate:make --knexfile ./src/knexfile.js --",
    "migrate:down": "knex migrate:down --knexfile ./src/knexfile.js --",
    "migrate:up": "knex migrate:up --knexfile ./src/knexfile.js --",
    "migrate:latest": "knex migrate:latest --knexfile ./src/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile ./src/knexfile.js",
    "migrate:live": "NODE_ENV=live knex migrate:latest --knexfile ./src/knexfile.js",
    "migrate:rollbacklive": "NODE_ENV=live knex migrate:rollback --knexfile ./src/knexfile.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@trys/thyme": "^2.0.1",
    "aws-sdk": "^2.273.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "html-to-text": "^4.0.0",
    "jsonwebtoken": "^8.3.0",
    "juice": "^4.3.2",
    "knex": "^0.19.4",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "multer-s3": "^2.7.0",
    "node-fetch": "^2.2.0",
    "nodemailer": "^4.4.2",
    "nodemon": "^1.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.3",
    "sanitize-html": "^1.18.2",
    "uuidv4": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "mocha": "^2.5.3",
    "rimraf": "^2.6.2"
  }
}
